{
  "expo": {
    "name": "Cosmix",
    "slug": "cosmix",
    "version": "1.0.0",
    "scheme": "cosmix",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#423120"
    },
    "sdkVersion": "54.0.0",
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-location",
      "expo-font",
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#423120",
          "image": "./assets/splash.png",
          "imageWidth": 200
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.cosmix.beauty",
          "enableGooglePay": false
        }
      ]
    ],
    "android": {
      "package": "com.cosmix.beauty",
      "versionCode": 1,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.INTERNET"
      ],
      "hardwareAccelerated": true,
      "softwareKeyboardLayoutMode": "resize",
      "usesCleartextTraffic": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "cosmix"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.cosmix.beauty",
      "buildNumber": "1"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b4d2be30-101f-4676-90b4-1aa0c95ec59f"
      }
    },
    "owner": "wkop"
  }
}
